{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "opener:default",
    "updater:default",
    "dialog:default",
    "fs:read-all",
    "fs:write-all",
    "fs:allow-mkdir",
    "fs:allow-read-dir",
    "fs:allow-home-read-recursive",
    "fs:allow-home-write-recursive"
  ]
}
